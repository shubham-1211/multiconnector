{
  "plugins": [],
  "protocol": "rest",
  "authConfig": {
    "url": "${url}",
    "headers": {
      "Authorization": "${apikey}"
    },
    "path": "/api/v1/Values/${interval}/${id}"
  },
  "dataObjects": [
    "values"
  ],
  "dataPropertyMappings": {
    "${type}": "value"
  },
  "generalConfig": {
    "query": {
      "start": "from",
      "end": "to"
    },
    "hardwareId": {
      "pathIndex": 5
    },
    "timestamp": {
      "dataObjectProperty": "timestamp"
    }
  }
}
