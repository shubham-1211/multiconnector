{
  "plugins": [
    "azure-service-bus"
  ],
  "protocol": "mqtt",
  "authConfig": {
    "cert": "${cert}",
    "key": "${key}",
    "url": "${url}",
    "path": "${id}",
    "topic": "${topic}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "MeasureAirTemperatureCelsiusDegree": "temp",
    "MeasureAirHumidityPercentage": "humd",
    "MeasureAirPressureMillibar": "airp",
    "MeasureLightLevelLux": "lght",
    "MeasureCount": "moveCount",
    "MeasureDistanceMilliMeter": "dist"
  },
  "generalConfig": {
    "sourceName": {
      "dataObjectProperty": "tsmId"
    },
    "hardwareId": {
      "dataObjectProperty": "tsmTuid"
    },
    "timestamp": {
      "dataObjectProperty": "tsmTs"
    }
  }
}
